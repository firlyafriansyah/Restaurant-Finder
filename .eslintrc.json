{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "no-underscore-dangle": [2, { "allowAfterThis": true }],
        "no-restricted-globals": ["error", "event", "fdescribe"]
    }
}
